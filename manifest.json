{
	"name": "Browse-Awesome",
	"version": "0.1",
	"description": "Creates awesome chrome browser shortcuts which lets you browse in a geeky way. It also allows you to create your own shortcuts.",
	"author": "Dheeraj Joshi",
	"manifest_version": 2,
	"background": {
		"persistent": false,
		"scripts": ["background.js"]
	},
	"commands": {
		"toggle-pin": {
			"suggested_key": {
				"default": "Alt+P"
			},
			"description": "Toggle pinned tab"
		},
		"move-tab-left": {
			"suggested_key": {
				"default": "Ctrl+Shift+Left"
			},
			"description": "Move tab to left"
		},
		"move-tab-right": {
			"suggested_key": {
				"default": "Ctrl+Shift+Right"
			},
			"description": "Move tab to right"
		}
	}
}
