{
	"name": "Browse-Awesome",
	"version": "0.1",
	"description": "Creates awesome chrome browser shortcuts which lets you browse in a geeky way. It also allows you to create your own shortcuts.",
	"author": "Dheeraj Joshi",
	"manifest_version": 2,
	"background": {
		"persistent": false,
		"scripts": ["background.js"]
	},
	"commands": {
		"toggle-pin": {
			"suggested_key": {
				"default": "Alt+P"
			},
			"description": "Toggle pinned tab"
		},
		"move-tab-left": {
			"suggested_key": {
				"default": "Ctrl+Shift+Left",
				"mac": "MacCtrl+Shift+Left"
			},
			"description": "Moves the current tab to left"
		},
		"move-tab-right": {
			"suggested_key": {
				"default": "Ctrl+Shift+Right",
				"mac": "MacCtrl+Shift+Right"
			},
			"description": "Moves the current tab to right"
		},
		"duplicate-tab": {
			"suggested_key": {
				"default": "Alt+D"
			},
			"description": "Duplicates the current tab"
		}
	},
	"icons": {
		"16": "icons/icon_16.png",
		"32": "icons/icon_32.png",
		"48": "icons/icon_48.png",
		"128": "icons/icon_128.png"
	}
}
